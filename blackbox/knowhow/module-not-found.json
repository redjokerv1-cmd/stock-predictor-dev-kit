{
  "category": "ModuleNotFoundError",
  "description": "Python에서 존재하지 않는 모듈 import 시 발생",
  "patterns": [
    {
      "id": "mod-001",
      "symptom": "No module named 'utils.xxx'",
      "causes": [
        "모듈명 오타",
        "존재하지 않는 모듈 import",
        "코드 복사 시 모듈명 확인 안함"
      ],
      "diagnosis_steps": [
        "1. ls utils/ 로 실제 모듈 목록 확인",
        "2. 올바른 모듈명으로 수정"
      ],
      "example": {
        "error": "No module named 'utils.naver_news'",
        "wrong": "from utils.naver_news import NaverNewsClient",
        "correct": "from utils.rss_parser import RSSParser",
        "context": "naver_news.py는 존재하지 않고 rss_parser.py가 같은 기능 수행"
      },
      "prevention": "debug-imports.py로 사전 검증"
    },
    {
      "id": "mod-002",
      "symptom": "No module named 'xxx' (외부 패키지)",
      "causes": [
        "requirements.txt에 없음",
        "가상환경 미활성화",
        "pip install 안됨"
      ],
      "diagnosis_steps": [
        "1. grep 'xxx' requirements.txt 로 확인",
        "2. pip install xxx 또는 requirements.txt에 추가",
        "3. Railway에서는 requirements.txt 자동 설치됨"
      ],
      "example": {
        "error": "No module named 'sqlalchemy'",
        "cause": "Sentry SqlalchemyIntegration 추가했지만 sqlalchemy 미설치",
        "solution": "SqlalchemyIntegration 제거 (사용하지 않으므로)"
      },
      "related_incident": "REFLECTION-002"
    },
    {
      "id": "mod-003",
      "symptom": "cannot import name 'X' from 'Y'",
      "causes": [
        "클래스/함수명 오타",
        "다른 버전의 모듈",
        "export 안됨"
      ],
      "diagnosis_steps": [
        "1. 해당 모듈 파일에서 실제 export 확인",
        "2. grep 'class X' 또는 grep 'def X'",
        "3. 올바른 이름으로 수정"
      ],
      "example": {
        "error": "cannot import name 'YouTubeAnalyzer' from 'utils.youtube_client'",
        "wrong": "from utils.youtube_client import YouTubeAnalyzer",
        "correct": "from utils.youtube_client import YoutubeClient",
        "note": "클래스명이 YoutubeClient (소문자 t)"
      }
    }
  ],
  "quick_fix_commands": [
    "ls utils/",
    "grep 'class ' utils/<module>.py",
    "grep 'def ' utils/<module>.py | head -20",
    "python -c 'from utils.xxx import yyy; print(\"OK\")'"
  ],
  "debug_tool": "python ../stock-predictor-dev-kit/tools/debug-imports.py",
  "related_rules": ["GEN-004", "LIB-001"],
  "updated": "2025-12-11"
}

